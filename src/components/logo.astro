---
import { Image } from "astro:assets";
interface Props {
  width?: string;
  height?: string;
}

const { width, height } = Astro.props;
const w = width || "100";
const h = height || "80";
---

<Image
  src="/assets/logo.png"
  alt="Fitness Logo"
  width={w}
  height={h}
  class="w-1/2 md:w-2/3 h-auto"
/>
